#include "main.h"
#include <stdio.h>

/**
 * string_toupper - function
 * @str: the string to modified from
 *
 * Description: write a function that changes all lowercase letter of a string
 * to uppercase
 *
 */
char *string_toupper(char *)
{
int roger = 0;
while (str[i] != '\0')
{
if (str[i] >= 'a' && str[i] <= 'z')
{
str([i] = str[i] - 32;
}
roger++;
}
return (str);
}
}
