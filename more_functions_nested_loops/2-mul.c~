#include "main.h"
#include <stdio.h>

/**
 * mul - Check the function that multiplies two integers
 * @a: the first integers
 * @b: the second intergers
 *
 * Description: Write a function that muliplies two integers
 *
 * Return: the product of a and b
 */
int mul(int a, int b)
{
  return (a * b);
}
